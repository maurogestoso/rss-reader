{
  "name": "react-router-template",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "db:studio": "npx dotenvx run -f .env.local -- npx drizzle-kit studio",
    "db:studio:prod": "npx dotenvx run -f .env.production -- npx drizzle-kit studio",
    "db:push": "npx dotenvx run -f .env.local -- npx drizzle-kit push",
    "db:push:prod": "npx dotenvx run -f .env.production -- npx drizzle-kit push"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@libsql/client": "^0.15.15",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "entities": "^7.0.0",
    "isbot": "^5.1.31",
    "lucide-react": "^0.548.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.2",
    "rss-parser": "^3.13.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/yargs": "^17.0.34",
    "drizzle-kit": "^0.31.5",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
